{
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "playwright"
  ],
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "declaration": true
  },
  "$schema": "https://json.schemastore.org/tsconfig"
}
